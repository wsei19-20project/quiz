<Window x:Class="Quizek.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Quizek"
        mc:Ignorable="d"
        Title="MainWindow" Height="600" Width="800" ResizeMode="NoResize" KeyUp="Window_KeyUp">

    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="61*"/>
            <ColumnDefinition Width="19*"/>
            <ColumnDefinition Width="0*"/>
        </Grid.ColumnDefinitions>
        <Grid.Background>
            <ImageBrush ImageSource="/images/grid-bg.jpg"/>
        </Grid.Background>
        <Frame x:Name="Main" NavigationUIVisibility="Hidden" Grid.ColumnSpan="2"/>


        <Image Height="232" Width="232" ForceCursor="True" Source="/Images/logo.png" VerticalAlignment="Top" HorizontalAlignment="Left" RenderTransformOrigin="0.5,0.5" Opacity="0.9" Margin="284,101,0,0" Panel.ZIndex="-1" UseLayoutRounding="True" />
        <TextBlock HorizontalAlignment="Left" Margin="279,316,0,0" Text="PRESS ANY KEY TO START" TextWrapping="Wrap" VerticalAlignment="Top" Foreground="#FF9692DE" FontSize="18" FontWeight="Bold" FontFamily="Unispace" Width="Auto" x:Name="presskey" />



    </Grid>
</Window>
